{
  "name": "foresry-starter-blog-gatsby",
  "private": true,
  "description": "A simple starter blog to get up and developing quickly with Forestry & Gatsby",
  "license": "MIT",
  "version": "0.1.0",
  "author": "kendall strautman",
  "scripts": {
    "build": "gatsby build",
    "dev": "tinacms dev -c 'gatsby develop -o'",
    "develop": "yarn dev",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn dev",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "forestry-preview": "gatsby develop -p 8080 -H 0.0.0.0"
  },
  "dependencies": {
    "@tinacms/cli": "1.5.12",
    "gatsby": "3.13.1",
    "gatsby-image": "3.0.0",
    "gatsby-plugin-react-helmet": "3.x.x",
    "gatsby-plugin-sass": "4.0.0",
    "gatsby-plugin-sharp": "3.x.x",
    "gatsby-remark-images": "4.x.x",
    "gatsby-remark-normalize-paths": "^1.1.0",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "3.x.x",
    "gatsby-transformer-remark": "3.x.x",
    "gatsby-transformer-sharp": "3.x.x",
    "gatsby-transformer-yaml": "3.x.x",
    "lightgallery": "2.7.0",
    "node-sass": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-masonry-css": "1.0.16",
    "tinacms": "1.5.5",
    "typescript": "^3.9.10",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "graphql": "^15.4.0",
    "graphql-compose": "^7.25.0",
    "webpack": "^5.52.1"
  },
